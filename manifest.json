{
  "manifest_version": 2,
  "name": "Stock Reports",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Enter Yahoo-Finance Stock Symbol"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
